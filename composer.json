{
    "name": "maelstrom-cms/toolkit",
    "description": "A CMS Toolkit built on Laravel and React",
    "type": "library",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "intervention/image": "^2.5",
        "laravel/framework": "5.*|6.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Owen Melbourne",
            "email": "owenmelbz@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Maelstrom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maelstrom\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maelstrom\\Providers\\MaelstromServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.2"
    }
}
